@model IEnumerable<RolesAuth.Models.CartItems>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CartFood_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cafe_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Quantity)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CartFood_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cafe_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CartFood_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CartFood_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CartFood_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
